{
  "name": "danmaku-components",
  "version": "1.0.0",
  "description": "跨框架实时弹幕渲染组件库，支持Vue、React和原生JavaScript",
  "type": "module",
  "main": "./dist/danmaku-components.umd.js",
  "module": "./dist/danmaku-components.es.js",
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "danmaku",
    "弹幕",
    "vue",
    "react",
    "realtime",
    "component",
    "library"
  ],
  "author": "",
  "license": "MIT",
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "vue": "^3.0.0"
  },
  "dependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.25",
    "@types/react": "^18.2.64",
    "@types/react-dom": "^18.2.21",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitejs/plugin-vue": "^5.0.4",
    "terser": "^5.44.0",
    "typescript": "^5.4.2",
    "vite": "^5.1.6",
    "vite-plugin-dts": "^3.7.3"
  },
  "exports": {
    ".": {
      "import": "./dist/danmaku-components.es.js",
      "require": "./dist/danmaku-components.umd.js"
    },
    "./vue": {
      "import": "./dist/vue/index.js",
      "require": "./dist/vue/index.cjs"
    },
    "./react": {
      "import": "./dist/react/index.js",
      "require": "./dist/react/index.cjs"
    },
    "./core": {
      "import": "./dist/core/index.js",
      "require": "./dist/core/index.cjs"
    }
  }
}
